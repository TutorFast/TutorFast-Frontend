  "license": "MIT",
  "scripts": {
    "build": "webpack --config dev.webpack.config.js",
    "server": "webpack-dev-server --config dev.webpack.config.js",
    "start:server": "rm -rf ./build && webpack --config webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "rm -rf ./.build && npm-run-all --parallel start:server start:client"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cjson": "^0.5.0",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "start-server-webpack-plugin": "^2.2.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-node-externals": "^1.6.0"
  }
